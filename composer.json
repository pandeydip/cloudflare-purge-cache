{
    "name": "pandeydip/cloudflare-purge-cache",
    "description": "This package helps you purge cahce specifying a host/url",
    "keywords": [
        "pandeydip",
        "laravel",
        "cloudflare-purge-cache"
    ],
    "homepage": "https://github.com/pandeydip/cloudflare-purge-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Deepak Pandey",
            "email": "er.pandeydip@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "illuminate/contracts": "^11.10.0",
        "spatie/laravel-package-tools": "^1.16.4"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pandeydip\\CloudflarePurgeCache\\": "src"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pandeydip\\CloudflarePurgeCache\\CloudflarePurgeCacheServiceProvider"
            ],
            "aliases": {
                "CloudflarePurgeCache": "Pandeydip\\CloudflarePurgeCache\\Facades\\CloudflarePurgeCache"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
